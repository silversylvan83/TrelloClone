{
  "name": "trello-clone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd src/server && npm run dev",
    "dev:client": "cd src/client && npm run dev",
    "build": "cd src/client && npm run build",
    "start": "node src/server/server.js"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
